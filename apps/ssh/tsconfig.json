{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "esModuleInterop": true,
    "incremental": true,
    "outDir": "dist",
    "rootDir": "src",
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
